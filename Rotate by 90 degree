void rotateby90(vector<vector<int> >& a, int n) 
    {
        for(int i=0;i<n;i++){
            
            for(int j=0;j<i;j++){
                swap(a[i][j],a[j][i]);
            }
            
        }
        
        for(int i =0;i<n/2;i++){
            for(int j=0;j<n;j++){
                swap(a[i][j],a[n-1-i][j]);
            }
        }
    }
        
        // code here 
    };
